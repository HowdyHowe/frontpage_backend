generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"  // Default location
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  user_id     String    @id @db.VarChar(10)
  username    String    @unique(map: "username_unique") @db.VarChar(99)
  password    String    @db.VarChar(99)

  created_at  DateTime  @default(now()) @db.Timestamp(6)
}